Algoritmo CONSTANTES_VARIABLES_PRACTICA
	
	definir LIMITE_EXTRACCION_DOLARES Como Entero;
	
	Escribir "LÍMITE EXTRACCIÓN EN  DÓLARES POR DÍA";
	
	LIMITE_EXTRACCION_DOLARES = 1000;
	
	Escribir  LIMITE_EXTRACCION_DOLARES;
	
	Leer  LIMITE_EXTRACCION_DOLARES;
	
	
	
	
	
	definir deposito_dolares Como Entero;
	
	Escribir "DEPÓSITO EN DÓLARES";
	
	Escribir deposito_dolares;
	
	leer deposito_dolares;
	
	escribir deposito_dolares;
	
	
	
FinAlgoritmo